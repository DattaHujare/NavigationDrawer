<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dattahujare.navigationdrwaerproject.fragment.ThirdFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp"
        android:orientation="vertical"
        android:padding="10dp">

        <Button
            android:id="@+id/btnFiveFrag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:layout_margin="3dp"
            android:text="Inner Five Fragment"
            android:textSize="20dp"
            android:textColor="@color/colorWhite"
            android:background="#a44c9a"/>
        <Button
            android:id="@+id/btnSixFrag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:layout_margin="3dp"
            android:text="Inner Six Fragment"
            android:textSize="20dp"
            android:textColor="@color/colorWhite"
            android:background="#a44c9a"/>


    </LinearLayout>

</FrameLayout>
